      "mongoose": "^8.9.0"
    },
    "devDependencies": {
      "@types/node": "^22.10.2",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "turbo": "^2.3.3",
      "typescript": "^5.7.2"
    }
  }